{{#if model.matured}}
<br/>
<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Height</th>
      <th>Block Hash</th>
      <th>Time Found</th>
      <th>Variance</th>
      <th>Reward</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.matured as |block|}}
      {{partial "blocks/block"}}
    {{/each}}
  </tbody>
</table>
{{else}}
    <br/>
    <table class="table-condensed table-striped">
      <thead>
        <tr>
          <th>No matured blocks yet</th>
        </tr>
      </thead>
    </table>
{{/if}}
